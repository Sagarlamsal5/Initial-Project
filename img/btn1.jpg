function vbCalcNightsMod757() {
	var vbcheckin = document.getElementById('checkindatemod757').value;
	var vbcheckout = document.getElementById('checkoutdatemod757').value;
	if (vbcheckin.length > 0 && vbcheckout.length > 0) {
